local selling_gui = {}

function selling_gui.add_history(table)
	local player_id = table.Player_ID
	local player_currency = table.Saved_Cash
	local player_sold_amount = table.Saved_Selling_Amount
	local player_name = table.Player_Name
	local player_display = table.Player_DisplayName
	local generated_name = table.Player_GeneratedName

    --[[
        
        Card

    --]]

	local card = Instance.new("Frame")
	card.AnchorPoint = Vector2.new(0.5, 0.5)
	card.BackgroundColor3 = Color3.new(0.113725, 0.113725, 0.113725)
	card.BorderColor3 = Color3.new(0, 0, 0)
