	uitext_size_constraint_5.Parent = remove

	local teleport = Instance.new("TextButton")
	teleport.Font = Enum.Font.GothamBold
	teleport.Text = "TP"
	teleport.TextColor3 = Color3.new(0.705882, 0.705882, 0.705882)
	teleport.TextScaled = true
	teleport.TextSize = 14
	teleport.TextWrapped = true
	teleport.AnchorPoint = Vector2.new(0.5, 0.5)
	teleport.BackgroundColor3 = Color3.new(0.0941176, 0.0941176, 0.0941176)
	teleport.BorderColor3 = Color3.new(0, 0, 0)
	teleport.BorderSizePixel = 0
	teleport.Position = UDim2.new(0.590514064, 0, 0.745211601, 0)
	teleport.Size = UDim2.new(0.180077031, 0, 0.388500243, 0)
	teleport.Visible = true
	teleport.Name = "Teleport"
	teleport.Parent = card

	local uicorner_4 = Instance.new("UICorner")
	uicorner_4.Parent = teleport

	local uitext_size_constraint_6 = Instance.new("UITextSizeConstraint")
	uitext_size_constraint_6.MaxTextSize = 8
	uitext_size_constraint_6.Parent = teleport

    --[[

    Functions

    --]]

	--> Important
	local begin_count = false
	local goal_amount = nil
	local completion_warning = false
	local connected = true
	local saved_name = player.Name

	--> Amount Checking
	local set_amount = player_currency.Value

	--> Player_Checks
	game.Players.PlayerAdded:Connect(function(plr)
		if saved_name == plr.Name then
			connected = true
			uistroke.Color = Color3.new(1, 0.341176, 0.341176)

			player_id = plr.UserId
			player_datafolder = plr:WaitForChild("DataFolder")
			player_currency = player_datafolder:WaitForChild("Currency")

			local amount_checking = player_currency:GetPropertyChangedSignal("Value"):Connect(function()
				if not connected then return end
		
				if begin_count then
		
					if completion_warning == false then
						if player_currency.Value > (goal_amount + set_amount) - ((goal_amount + set_amount) * 0.20) then
							-- Player has reached 80% of their goal.
							completion_warning = true
		
							if loaded_data["SoundID"] ~= nil then
								local sound = Instance.new("Sound")
								sound.SoundId = "rbxassetid://136075117"
								sound.PlayOnRemove = true
								sound.Parent = workspace
								sound:Destroy()
							end
		
							if loaded_data["Webhook"] ~= nil then
								local request = http_request or httprequest or error("Webhook API: Failed to find HTTP request function");
								local date_time = "<t:"..os.time()..":R>"
								local blurred_name = string.sub(player.Name, 1, 3)..string.gsub(string.sub(player.Name, 4, #player.Name), ".", "*")
			
								local content = {
									content = "<@744274804135428180>",
									embeds = {
										{	
											title = "Purchase Near Complete",
											description = "User ``"..blurred_name.."`` has collected 80% of their order of ``"..tostring("$"..format(goal_amount)).."``\nOrder Warning: "..date_time,
											url = "",
											color = 16776960,
										}
									},
									attachments = {}
								}
			
								local response = request(
									{
										Url = loaded_data["Webhook"],
										Method = "POST",
										Headers = {
											["Content-Type"] = "application/json"
										},
										Body = HttpService:JSONEncode(content)
									}
								)
							end
						end
					end
		
					if set_amount > player_currency.Value then
						-- player has spent money
		
						goal_amount -= set_amount - player_currency.Value
						set_amount = player_currency.Value
						goal.Text = "$"..format(goal_amount)
					elseif player_currency.Value > (goal_amount + set_amount) then
						-- player complete
						begin_count = false
						uistroke.Color = Color3.fromRGB(0, 255, 0)
		
						if loaded_data["SoundID"] ~= nil then
							local sound = Instance.new("Sound")
							sound.SoundId = loaded_data["SoundID"]
							sound.PlayOnRemove = true
							sound.Parent = workspace
							sound:Destroy()
						end
		
						if loaded_data["Webhook"] ~= nil then
							local request = http_request or httprequest or error("Webhook API: Failed to find HTTP request function");
							local date_time = "<t:"..os.time()..":R>"
							local blurred_name = string.sub(player.Name, 1, 3)..string.gsub(string.sub(player.Name, 4, #player.Name), ".", "*")
		
							local content = {
								content = "<@489201683654508544>",
								embeds = {
									{	
										title = "Purchase Complete",
										description = "User ``"..blurred_name.."`` has collected their order of ``"..tostring("$"..format(goal_amount)).."``\nOrder Date: "..date_time,
										url = "",
										color = 50000,
									}
								},
								attachments = {}
							}
		
							local response = request(
								{
									Url = loaded_data["Webhook"],
									Method = "POST",
									Headers = {
										["Content-Type"] = "application/json"
									},
									Body = HttpService:JSONEncode(content)
								}
							)
						end
					end
				else
					set_amount = player_currency.Value
				end
		
				-- update cash label
				cash.Text = "$"..format(tostring(player_currency.Value))
			end)
		
			--> Input Detection
			local textbox_checking = cash_to_pickup.FocusLost:Connect(function()
				if not connected then return end
		
				-- Amount Has Been Set (Checking)
		
				local amount = cash_to_pickup.Text
				if tonumber(amount) == nil then --> Contains A Shortcut
					local shortcutted = shortcut(amount)
		
					if shortcutted then
						goal_amount = tonumber(shortcutted)
						cash_to_pickup.Text = "$"..format(tostring(goal_amount))
						goal.Text = "$"..format(goal_amount)
		
						begin_count = true
					else
						warn("A Error Has Occured When Setting Shortcut!")
					end
				else --> Doesn't Contain Shortcut
					goal_amount = tonumber(cash_to_pickup.Text)
					cash_to_pickup.Text = "$"..format(tostring(goal_amount))
					goal.Text = "$"..format(tostring(goal_amount + set_amount))
					begin_count = true
		
					if not goal_amount then
						return warn("Failed To Set Goal Amount")
					end
				end
			end)
		end
	end)

	game.Players.PlayerRemoving:Connect(function(plr)
		print(plr)
		if plr == player then
			connected = false
			uistroke.Color = Color3.new(0.572549, 0.341176, 1)
		end
	end)

	local amount_checking = player_currency:GetPropertyChangedSignal("Value"):Connect(function()
		if not connected then return end

		if begin_count then

			if completion_warning == false then
				if player_currency.Value > (goal_amount + set_amount) - ((goal_amount + set_amount) * 0.20) then
					-- Player has reached 80% of their goal.
					completion_warning = true

					if loaded_data["SoundID"] ~= nil then
						local sound = Instance.new("Sound")
						sound.SoundId = "rbxassetid://136075117"
						sound.PlayOnRemove = true
						sound.Parent = workspace
						sound:Destroy()
					end

					if loaded_data["Webhook"] ~= nil then
						local request = http_request or httprequest or error("Webhook API: Failed to find HTTP request function");
						local date_time = "<t:"..os.time()..":R>"
						local blurred_name = string.sub(player.Name, 1, 3)..string.gsub(string.sub(player.Name, 4, #player.Name), ".", "*")
	
						local content = {
							content = "<@489201683654508544>",
							embeds = {
								{	
									title = "Purchase Near Complete",
									description = "User ``"..blurred_name.."`` has collected 80% of their order of ``"..tostring("$"..format(goal_amount)).."``\nOrder Warning: "..date_time,
									url = "",
									color = 16776960,
								}
							},
							attachments = {}
						}
	
						local response = request(
							{
								Url = loaded_data["Webhook"],
								Method = "POST",
								Headers = {
									["Content-Type"] = "application/json"
								},
								Body = HttpService:JSONEncode(content)
							}
						)
					end
				end
			end

			if set_amount > player_currency.Value then
				-- player has spent money

				goal_amount -= set_amount - player_currency.Value
				set_amount = player_currency.Value
				goal.Text = "$"..format(goal_amount)
			elseif player_currency.Value > (goal_amount + set_amount) then
				-- player complete
                begin_count = false
				uistroke.Color = Color3.fromRGB(0, 255, 0)

                if loaded_data["SoundID"] ~= nil then
                    local sound = Instance.new("Sound")
                    sound.SoundId = loaded_data["SoundID"]
                    sound.PlayOnRemove = true
                    sound.Parent = workspace
                    sound:Destroy()
                end

                if loaded_data["Webhook"] ~= nil then
                    local request = http_request or httprequest or error("Webhook API: Failed to find HTTP request function");
                    local date_time = "<t:"..os.time()..":R>"
                    local blurred_name = string.sub(player.Name, 1, 3)..string.gsub(string.sub(player.Name, 4, #player.Name), ".", "*")

					local content = {
						content = "<@489201683654508544>",
						embeds = {
							{	
								title = "Purchase Complete",
								description = "User ``"..blurred_name.."`` has collected their order of ``"..tostring("$"..format(goal_amount)).."``\nOrder Date: "..date_time,
								url = "",
								color = 50000,
							}
						},
						attachments = {}
					}

                    local response = request(
                        {
                            Url = loaded_data["Webhook"],
                            Method = "POST",
                            Headers = {
                                ["Content-Type"] = "application/json"
                            },
                            Body = HttpService:JSONEncode(content)
                        }
                    )
                end
			end
		else
			set_amount = player_currency.Value
		end

		-- update cash label
		cash.Text = "$"..format(tostring(player_currency.Value))
	end)

	--> Input Detection
	local textbox_checking = cash_to_pickup.FocusLost:Connect(function()
		if not connected then return end

		-- Amount Has Been Set (Checking)

		local amount = cash_to_pickup.Text
		if tonumber(amount) == nil then --> Contains A Shortcut
			local shortcutted = shortcut(amount)

			if shortcutted then
				goal_amount = tonumber(shortcutted)
				cash_to_pickup.Text = "$"..format(tostring(goal_amount))
				goal.Text = '$'..format(tostring(tonumber(goal_amount + tonumber(player_currency.Value))))

				begin_count = true
			else
				warn("A Error Has Occured When Setting Shortcut!")
			end
		else --> Doesn't Contain Shortcut
			goal_amount = tonumber(cash_to_pickup.Text)
			cash_to_pickup.Text = "$"..format(tostring(goal_amount))
			goal.Text = "$"..format(tostring(goal_amount + set_amount))
			begin_count = true

			if not goal_amount then
				return warn("Failed To Set Goal Amount")
			end
		end
	end)

	--> Teleport
	local teleport_card = teleport.MouseButton1Down:Connect(function()
		if not connected then return end

		if player.Character then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players:WaitForChild(saved_name).Character.HumanoidRootPart.CFrame
		end
	end)

	--> Remove Card
	local remove_card = remove.MouseButton1Down:Connect(function()
		amount_checking:Disconnect()
		textbox_checking:Disconnect()

		local loaded_data = HttpService:JSONDecode(readfile("CloneSellingGui.txt"))
		local generated_name = tostring(HttpService:GenerateGUID())
        loaded_data["HistoryTab"][generated_name] = {
			Player_ID = player.UserId,
			Player_DisplayName = player.DisplayName,
			Player_Name = player.Name,
			Saved_Cash = tostring("$"..format(tostring(player_currency.Value))),
			Saved_Selling_Amount = "$"..format(tostring(goal_amount)),
			Player_GeneratedName = generated_name
		}

        local encoded = HttpService:JSONEncode(loaded_data)
        delfile("CloneSellingGui.txt")
        writefile("CloneSellingGui.txt", encoded)

		card:Destroy()
	end)
end

--[[

Page Functions

--]]

main.MouseButton1Down:Connect(function()
	main_2.Visible = true
	history_2.Visible = false
	utilities_2.Visible = false
	settings_2.Visible = false
end)

history.MouseButton1Down:Connect(function()
	main_2.Visible = false
	history_2.Visible = true
	utilities_2.Visible = false
	settings_2.Visible = false

	for _, card in ipairs(history_2:GetChildren()) do
		if card:IsA("Frame") then
			card:Destroy()
		end
	end

	local HttpService = game:GetService("HttpService")
	local loaded_data = HttpService:JSONDecode(readfile("CloneSellingGui.txt"))
	for _, data in pairs(loaded_data["HistoryTab"]) do
		selling_gui.add_history(data)
	end
end)

utilities.MouseButton1Down:Connect(function()
	main_2.Visible = false
	history_2.Visible = false
	utilities_2.Visible = true
	settings_2.Visible = false
end)

settings.MouseButton1Down:Connect(function()
	main_2.Visible = false
	history_2.Visible = false
	utilities_2.Visible = false
	settings_2.Visible = true
end)

return selling_gui
