	uitext_size_constraint.Parent = cash

	local goal = Instance.new("TextLabel")
	goal.Font = Enum.Font.Gotham
	goal.Text = player_sold_amount
	goal.TextColor3 = Color3.new(1, 1, 1)
	goal.TextScaled = true
