--[[

Important Functions

--]]

--> Webhook Saving
save_settings.MouseButton1Down:Connect(function()
    warn("Saving Webhook To: "..webhook_3.Text)
    warn("Saving SoundID To: "..sound_id.Text)

    loaded_data["Webhook"] = webhook_3.Text
    loaded_data["SoundID"] = sound_id.Text
    local encoded = HttpService:JSONEncode(loaded_data)

    delfile("CloneSellingGui.txt")
    writefile("CloneSellingGui.txt", encoded)

    local loaded_data = HttpService:JSONDecode(readfile("CloneSellingGui.txt"))
end)

--> Number Formatter
local function format(number)
	number = tostring(number)
	return number:reverse():gsub("...","%0,",math.floor((#number-1)/3)):reverse()
end

--> Remove Numbers
local function remove_numbers(x)
	return ({x:gsub("%d", "")})[1]
end

--> String Shortcuts
local function shortcut(number, shortcut)
	local number_amount = tonumber(number:sub(1, #number - 1))
	local shortcut_value = remove_numbers(tostring(number))

	if shortcut_value == "k" then
		number_amount *= 1000
	elseif shortcut_value == "m" then
		number_amount *= 1000000
	elseif shortcut_value == "b" then
		number_amount *= 1000000000
	end

	return number_amount
end

--> Drag
canvas_group.Draggable = true
canvas_group.Active = true
canvas_group.Selectable = true

--> Unload
unload.MouseButton1Down:Connect(function()
	canvas_group:Destroy()
end)

