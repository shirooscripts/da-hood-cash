	remove.Parent = card

	local uicorner_3 = Instance.new("UICorner")
	uicorner_3.Parent = remove

	local uitext_size_constraint_5 = Instance.new("UITextSizeConstraint")
	uitext_size_constraint_5.MaxTextSize = 8
	uitext_size_constraint_5.Parent = remove

	local remove_history = remove.MouseButton1Down:Connect(function()
        local loaded_data = HttpService:JSONDecode(readfile("CloneSellingGui.txt"))
        loaded_data["HistoryTab"][generated_name] = nil
        local encoded = HttpService:JSONEncode(loaded_data)
        delfile("CloneSellingGui.txt")
        writefile("CloneSellingGui.txt", encoded)

		card:Destroy()
	end)
end

function selling_gui.card_exists(player)
	if main_2:FindFirstChild(player.Name) then
		return true
	else
		return false
	end
end

