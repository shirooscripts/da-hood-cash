	card.Size = UDim2.new(0.99999994, 0, 0.106357202, 0)
	card.Visible = true
	card.Name = player_name
	card.Parent = history_2

	local player_icon = Instance.new("ImageLabel")
	player_icon.Image = "https://www.roblox.com/bust-thumbnail/image?userId="..tostring(player_id).."&width=420&height=420&format=png"
	player_icon.ImageColor3 = Color3.new(255, 255, 255)
	player_icon.AnchorPoint = Vector2.new(0.5, 0.5)
	player_icon.BackgroundColor3 = Color3.new(1, 1, 1)
	player_icon.BackgroundTransparency = 1
	player_icon.BorderColor3 = Color3.new(0, 0, 0)
	player_icon.BorderSizePixel = 0
	player_icon.Position = UDim2.new(0.0843881518, 0, 0.497655749, 0)
	player_icon.Size = UDim2.new(0.105989054, 0, 0.626111567, 0)
	player_icon.Visible = true
	player_icon.Name = "History"
	player_icon.Parent = card

	local uicorner = Instance.new("UICorner")
	uicorner.Parent = player_icon

	local uistroke = Instance.new("UIStroke")
	uistroke.Color = Color3.new(1, 0.341176, 0.341176)
	uistroke.Thickness = 2
	uistroke.Parent = player_icon

	local cash = Instance.new("TextLabel")
	cash.Font = Enum.Font.Gotham
	cash.Text = player_currency
	cash.TextColor3 = Color3.new(1, 1, 1)
	cash.TextScaled = true
	cash.TextSize = 14
