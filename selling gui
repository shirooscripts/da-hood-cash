local HttpService = game:GetService("HttpService")
if not isfile("CloneSellingGui.txt") then
	local loaded_data = {
		["Webhook"] = "",
		["SoundID"] = "",
		["HistoryTab"] = {

		}
	}

	local content = HttpService:JSONEncode(loaded_data)
	writefile("CloneSellingGui.txt", content)
end

local loaded_data = HttpService:JSONDecode(readfile("CloneSellingGui.txt"))

