	username.BorderSizePixel = 0
	username.Position = UDim2.new(0.306140333, 0, 0.306648135, 0)
	username.Size = UDim2.new(0.271894187, 0, 0.271533161, 0)
	username.Visible = true
	username.Name = "Username"
	username.Parent = card

	local uitext_size_constraint_4 = Instance.new("UITextSizeConstraint")
	uitext_size_constraint_4.MaxTextSize = 14
	uitext_size_constraint_4.Parent = username

	local remove = Instance.new("TextButton")
	remove.Font = Enum.Font.GothamBold
	remove.Text = "REMOVE HISTORY"
	remove.TextColor3 = Color3.new(1, 0, 0)
	remove.TextScaled = true
	remove.TextSize = 14
	remove.TextWrapped = true
	remove.AnchorPoint = Vector2.new(0.5, 0.5)
	remove.BackgroundColor3 = Color3.new(0.0941176, 0.0941176, 0.0941176)
	remove.BorderColor3 = Color3.new(0, 0, 0)
	remove.BorderSizePixel = 0
	remove.Position = UDim2.new(0.590514064, 0, 0.306250215, 0)
	remove.Size = UDim2.new(0.180077031, 0, 0.388500214, 0)
	remove.Visible = true
